{
    "version": 2,
    "builds": [
      {
        "src": "./Server/index.js",  
        "use": "@vercel/node"
      },
      {
        "src": "./Client/dist",    
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/Server/index.js"
      }
    ]
  }